          <titles>
            <title>Convective blueshifts in the solar atmosphere</title>
            <subtitle>III. High-accuracy observations of spectral lines in the visible⋆</subtitle>
          </titles>
          <contributors>
            <person_name sequence="first" contributor_role="author">
              <given_name>J.</given_name>
              <surname>Löhner-Böttcher</surname>
              <ORCID>http://orcid.org/0000-0002-4618-4365</ORCID>
            </person_name>
